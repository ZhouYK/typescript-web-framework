{
  "name": "typescript-web-framework",
  "version": "0.0.1",
  "description": "typescript 脚手架",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.less": [
      "stylelint --fix --syntax less"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ],
    "test/": [
      "npm run test"
    ]
  },
  "scripts": {
    "test": "jest --no-cache",
    "start": "webpack-dev-server --hot --config ./webpack/webpack.config.dev.babel.js",
    "build": "webpack --env=local --config  ./webpack/webpack.config.babel.js",
    "build:prod": "webpack --env=prod --config  ./webpack/webpack.config.babel.js",
    "build:stage": "webpack --env=stage --config  ./webpack/webpack.config.babel.js",
    "build:test": "webpack --env=test --config  ./webpack/webpack.config.babel.js",
    "build:dev": "webpack --env=dev --config  ./webpack/webpack.config.babel.js"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true,
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/test/tsconfig.test.json"
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 10 version",
    "not dead",
    "ie >= 9"
  ],
  "dependencies": {
    "antd": "~3.20.7",
    "axios": "~0.21.1",
    "classnames": "~2.2.6",
    "femo": "~1.0.0-alpha.0",
    "path-to-regexp": "~3.0.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-router": "~5.1.2",
    "react-router-dom": "~5.1.2",
    "typescript": "~3.4.5"
  },
  "devDependencies": {
    "@babel/cli": "~7.4.4",
    "@babel/core": "~7.4.4",
    "@babel/plugin-proposal-class-properties": "~7.2.1",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/plugin-transform-runtime": "~7.4.4",
    "@babel/preset-env": "~7.4.4",
    "@babel/preset-react": "~7.0.0",
    "@babel/register": "~7.0.0",
    "@babel/runtime-corejs2": "~7.4.4",
    "@babel/runtime-corejs3": "~7.5.5",
    "@types/classnames": "~2.2.9",
    "@types/react": "~16.9.34",
    "@types/react-dom": "~16.9.6",
    "@types/react-router": "~5.1.4",
    "@types/react-router-dom": "~5.1.3",
    "@typescript-eslint/eslint-plugin": "~1.6.0",
    "@typescript-eslint/parser": "~1.6.0",
    "autoprefixer": "~9.4.10",
    "babel-loader": "~8.0.5",
    "babel-plugin-import": "~1.12.2",
    "clean-webpack-plugin": "~0.1.19",
    "cross-env": "~5.2.1",
    "css-loader": "~0.28.11",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-config-prettier": "~4.1.0",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-loader": "~2.1.2",
    "eslint-plugin-import": "~2.16.0",
    "eslint-plugin-jest": "~22.4.1",
    "eslint-plugin-react": "~7.12.4",
    "eslint-plugin-react-hooks": "~1.6.0",
    "hard-source-webpack-plugin": "~0.13.1",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~1.3.1",
    "jest": "~24.5.0",
    "less": "~3.9.0",
    "less-loader": "~4.1.0",
    "lint-staged": "~8.1.5",
    "mini-css-extract-plugin": "~0.5.0",
    "optimize-css-assets-webpack-plugin": "~5.0.1",
    "postcss-loader": "~3.0.0",
    "progress-bar-webpack-plugin": "~1.11.0",
    "style-loader": "~0.23.1",
    "stylelint": "~9.10.1",
    "stylelint-config-standard": "~18.2.0",
    "ts-jest": "~24.0.0",
    "ts-loader": "~5.3.3",
    "uglifyjs-webpack-plugin": "~2.1.2",
    "webpack": "~4.28.4",
    "webpack-cli": "~3.2.3",
    "webpack-dev-server": "~3.2.1",
    "webpack-manifest-plugin": "~2.0.4"
  }
}
